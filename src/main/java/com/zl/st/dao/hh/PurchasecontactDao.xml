<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zl.st.dao.hh.PurchasecontactDao" >
	<select id="findContactByContactId" parameterType="java.lang.Integer" resultType="com.zl.st.pojo.hh.Purchasecontact">
		select purchaserid, username, userpwd, nickname, buyersid, telphone, fulladdress
		from purchasecontact where purchaserid=#{purchaserid}
	</select>
	
	


</mapper>