<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zl.st.dao.yk.BiddingMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zl.st.pojo.Bidding">
        <id column="bidding_id" property="biddingId" />
        <result column="supplier_id" property="supplierId" />
        <result column="supplier_comm_id" property="supplierCommId" />
        <result column="supplier_price" property="supplierPrice" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        bidding_id, supplier_id, supplier_comm_id, supplier_price
    </sql>

</mapper>
